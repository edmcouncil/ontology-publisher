      </spl:Argument>
    </spin:constraint>
    <rdfs:subClassOf rdf:resource="http://spinrdf.org/spl#StringFunctions"/>
    <spin:returnType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
