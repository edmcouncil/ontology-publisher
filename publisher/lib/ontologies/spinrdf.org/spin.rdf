    <!-- http://spinrdf.org/spin#AskTemplate -->

    <owl:Class rdf:about="http://spinrdf.org/spin#AskTemplate">
        <rdfs:subClassOf rdf:resource="http://spinrdf.org/spin#Template"/>
        <constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Attribute"/>
                <spl:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:maxCount>
                <spl:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</spl:minCount>
                <spl:predicate rdf:resource="http://spinrdf.org/spin#body"/>
                <spl:valueType rdf:resource="http://spinrdf.org/sp#Ask"/>
                <rdfs:comment>The ASK query wrapped by this template.</rdfs:comment>
            </rdf:Description>
        </constraint>
        <rdfs:comment>A SPIN template that wraps an ASK query.</rdfs:comment>
        <rdfs:label>Ask template</rdfs:label>
    </owl:Class>
    


