    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/classifies -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/classifies">
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isClassifiedBy"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>classifies</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">arranges in categories according to shared characteristics</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasPart -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasPart">
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isPartOf"/>
        <sm:directSource rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stanford Encyclopedia of Philosophy at http://plato.stanford.edu/entries/mereology/</sm:directSource>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has part</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates any portion of something, regardless of whether the portion itself is attached to the remainder or detached; cognitively salient or arbitrarily demarcated; self-connected or disconnected; homogeneous or gerrymandered; material or immaterial; extended or unextended; spatial or temporal</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasSubregion -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasSubregion">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasPart"/>
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isSubregionOf"/>
        <rdfs:domain rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has subregion</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates a geographic region to another geographic region that is a designated subregion of it</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isClassifiedBy -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isClassifiedBy">
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>is classified by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates the classifier used to characterize something</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isPartOf -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isPartOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <sm:directSource rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stanford Encyclopedia of Philosophy at http://plato.stanford.edu/entries/mereology/</sm:directSource>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>is a part of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates something to another thing that it is some component or portion of, regardless of how that whole-part relationship is manifested, i.e., attached to the remainder or detached; cognitively salient or arbitrarily demarcated; self-connected or disconnected; homogeneous or gerrymandered; material or immaterial; extended or unextended; spatial or temporal; the most generic part relation, reflexive, asymmetric, and transitive</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isSubregionOf -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isSubregionOf">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isPartOf"/>
        <rdfs:domain rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeographicRegion"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>is a subregion of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates a geographic region to another geographic region that it is a part of</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isUsedBy -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/isUsedBy">
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/uses"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>is used by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is employed in the process of accomplishing something</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/uses -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/uses">
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>uses</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">employs as a means of accomplishing some task or achieving some result</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/usesAdministrativeLanguage -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/usesAdministrativeLanguage">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/uses"/>
        <rdfs:domain rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeopoliticalEntity"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>uses administrative language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates a country or geopolitical entity to an administrative language that entity uses for international communications</skos:definition>
    </owl:ObjectProperty>
    


