        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a bibliographic reference for and/or URL of any electronic files or documents (e.g., document version URL if applicable) on which this specification or model depends; if the model contains multiple modules, then dependencies should reflect those of the model in question</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/directSource -->

