        <rdfs:label>contributor</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">an organization or individual that has made a significant contribution to the specification</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/copyright -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/copyright">
        <rdfs:label>copyright</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">copyright statement for the specification version, content, or model/electronic file</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/rights"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/dependsOn -->

