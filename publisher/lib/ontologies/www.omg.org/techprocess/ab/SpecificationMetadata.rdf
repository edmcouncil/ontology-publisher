    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/author -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/author">
        <rdfs:label>author</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author identifies an individual or organization that originated the model or specification (range can be a dct:Agent or string).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/creator"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/contentLanguage -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/contentLanguage">
        <rdfs:label>content language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a link to the core language and/or specification of the model/electronic file, for example, UML, OWL, SysML, BPMN, including linking to the version of the language in question</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/type"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/contentType -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/contentType">
        <rdfs:label>content type</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates the content type, for example, MOF metamodel, UML profile, UML model, SBVR or RDF vocabulary, OWL or CL ontology</skos:definition>
        <rdfs:range rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/TypeOfContent"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/contributor -->

