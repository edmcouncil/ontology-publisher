    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/addressForComments -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/addressForComments">
        <rdfs:label>addressForComments</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a link to a mailing list or web page where questions, discussion, and issues can be raised</skos:definition>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:domain rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/SpecificationVersion"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.omg.org/techprocess/ab/SpecificationMetadata/affectsModelElement -->

    <owl:AnnotationProperty rdf:about="http://www.omg.org/techprocess/ab/SpecificationMetadata/affectsModelElement">
