                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The name of an output variable that will contain the root object (in RDF).</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


