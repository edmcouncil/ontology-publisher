    <!-- http://topbraid.org/sparqlmotionlib#ApplyTopSPIN -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#ApplyTopSPIN">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:defaultValue rdf:resource="http://spinrdf.org/spin#rule"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#predicate"/>
                <spl:valueType rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The predicate to get the rules from. Usually spin:rule, but can be any SPIN property (subPropertyOf spin:query)</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#singlePass"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies whether only a single pass over all rules shall be made. By default (false), TopSPIN will iterate until no further inferences have been made.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#replace"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies whether the resulting infererred triples shall replace the input. If false, the union of inferred triples and old triples are returned.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#AssertTrue -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#AssertTrue">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Attribute"/>
                <spl:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:maxCount>
                <spl:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:minCount>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#askQuery"/>
                <spl:valueType rdf:resource="http://spinrdf.org/sp#Ask"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The ASK query that must return true.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
