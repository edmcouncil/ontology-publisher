    <!-- http://spinrdf.org/spl#Argument -->

    <rdf:Description rdf:about="http://spinrdf.org/spl#Argument">
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#hidden"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicates whether this is a &quot;hidden&quot; argument. Hidden arguments will not be presented to the user in input dialogs but instead always have their defaultValue.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </rdf:Description>
    


    <!-- http://topbraid.org/sparqlmotionlib#AppendText -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#AppendText">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#template"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The (template) string to append to the variable value.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:defaultValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">text</spl:defaultValue>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#variable"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The name of the variable to append to.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#ApplyConstruct -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#ApplyConstruct">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#replace"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If set to true, the output triples will only contain the constructed triples. If no values or false are specified, the output will be the union of the input triples and the constructed triples.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Attribute"/>
                <spl:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:minCount>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#constructQuery"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The SPARQL Construct queries that deliver the triples that shall be added.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#ApplyJenaInferencing -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#ApplyJenaInferencing">
