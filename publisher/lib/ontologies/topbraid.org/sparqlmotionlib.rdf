    <!-- http://topbraid.org/sparqlmotionlib#CreateTextFromJSP -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#CreateTextFromJSP">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:defaultValue>text</spl:defaultValue>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotion#outputVariable"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The name of the variable that will hold the output of the JSP as an xsd:string. Default: text.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
