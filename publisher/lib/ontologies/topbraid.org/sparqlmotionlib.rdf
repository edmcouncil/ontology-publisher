                <spl:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</spl:maxCount>
                <spl:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</spl:minCount>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#selectQuery"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A SPARQL Select query that binds instances of email:Message to send out.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#SerializeSPARQLResults -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#SerializeSPARQLResults">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:defaultValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">text</spl:defaultValue>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotion#outputVariable"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The name of the output variable that will contain the result.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:defaultValue rdf:resource="http://topbraid.org/sparqlmotion#XML"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#serialization"/>
                <spl:valueType rdf:resource="http://topbraid.org/sparqlmotion#ValueType"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The serialization format.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Attribute"/>
                <spl:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:maxCount>
                <spl:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</spl:minCount>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#selectQuery"/>
                <spl:valueType rdf:resource="http://spinrdf.org/sp#Select"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The SELECT query to execute.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#TextProcessingModules -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#TextProcessingModules">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#TrackChanges -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#TrackChanges">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:optional rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</spl:optional>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#onCommit"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Can be used (in TopBraid Live only) to specify that a script shall be executed after commits, and not after edits. In TBC this is ignored, and script will always be executed after edits.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


    <!-- http://topbraid.org/sparqlmotionlib#TrackEMails -->

    <owl:NamedIndividual rdf:about="http://topbraid.org/sparqlmotionlib#TrackEMails">
        <rdf:type rdf:resource="http://topbraid.org/sparqlmotion#Module"/>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#emailAccount"/>
                <spl:valueType rdf:resource="http://topbraid.org/email#Address"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The email account to track messages at.</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
        <spin:constraint>
            <rdf:Description>
                <rdf:type rdf:resource="http://spinrdf.org/spl#Argument"/>
                <spl:predicate rdf:resource="http://topbraid.org/sparqlmotionlib#path"/>
                <spl:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The path to the folder to get (e.g. &quot;INBOX&quot;)</rdfs:comment>
            </rdf:Description>
        </spin:constraint>
    </owl:NamedIndividual>
    


